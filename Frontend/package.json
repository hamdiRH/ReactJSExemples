{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "setupFiles": [
    "jest-prop-type-error"
  ],
  "engines": {
    "npm": "=6.14.9"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "build:doc": "react-doc-creator",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env node server",
    "dev": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production PORT=3000 node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test:jest": "jest",
    "test": "npm run test:jest ",
    "pretest": "rm -rf .nyc_output || true",
    "posttest": "npm run report:combined",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "npm run mkdir:reports",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true",
    "combine:reports": "nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "nyc report --reporter lcov --report-dir coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js,!app/pages/Graphnos/GraphContainer/**/*.js": [
      "prettier --write",
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "antd": "^4.16.13",
    "axios": "^0.23.0",
    "bootstrap": "5.2.0-beta1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "connected-react-router": "^6.9.1",
    "cross-env": "^7.0.3",
    "draft-js": "^0.11.7",
    "express": "^4.17.1",
    "fontfaceobserver": "^2.1.0",
    "history": "4.10.1",
    "hoist-non-react-statics": "^3.3.2",
    "immer": "^9.0.6",
    "initials": "^3.1.1",
    "intl": "^1.2.5",
    "invariant": "^2.2.4",
    "ip": "^1.1.5",
    "jodit-react": "^1.1.5",
    "jsondiffpatch": "^0.4.1",
    "jwt-decode": "^3.1.2",
    "less": "^4.1.2",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "prop-types": "15.7.2",
    "react": "^17.0.2",
    "react-cookie-consent": "^6.4.1",
    "react-dom": "17.0.2",
    "react-draft-wysiwyg": "^1.14.7",
    "react-helmet": "6.1.0",
    "react-hotkeys": "^2.0.0",
    "react-html-parser": "^2.0.2",
    "react-intl": "5.20.13",
    "react-redux": "7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "4.1.1",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "sanitize.css": "13.0.0",
    "sass": "^1.43.2",
    "sass-loader": "12",
    "styled-components": "5.3.1",
    "url-parse": "^1.5.3",
    "uuid": "^8.3.2",
    "webpack": "^5.58.2"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.15.4",
    "@babel/plugin-transform-react-constant-elements": "7.14.5",
    "@babel/plugin-transform-react-inline-elements": "7.14.5",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/register": "7.15.3",
    "add-asset-html-webpack-plugin": "3.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "8.2.25",
    "babel-plugin-styled-components": "1.13.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "^5.2.2",
    "compare-versions": "3.6.0",
    "compression-webpack-plugin": "^9.0.0",
    "coveralls": "3.1.1",
    "css-loader": "6.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-redux-saga": "1.2.1",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "^5.4.0",
    "image-webpack-loader": "8.0.1",
    "imports-loader": "3.0.0",
    "jest": "^27.2.5",
    "jest-cli": "27.2.5",
    "jest-dom": "4.0.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-styled-components": "^7.0.5",
    "less-loader": "^10.1.0",
    "lint-staged": "11.2.3",
    "ngrok": "4.2.2",
    "node-plop": "^0.26.2",
    "null-loader": "4.0.1",
    "offline-plugin": "5.0.7",
    "plop": "^2.7.4",
    "pre-commit": "1.2.2",
    "prettier": "2.4.1",
    "react-app-polyfill": "2.0.0",
    "react-doc-creator": "0.0.9",
    "react-docgen": "^5.4.0",
    "react-test-renderer": "17.0.2",
    "react-testing-library": "^8.0.1",
    "rimraf": "3.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "3.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "4.1.1",
    "webpack-cli": "4.9.0",
    "webpack-dev-middleware": "5.2.1",
    "webpack-hot-middleware": "2.25.1",
    "webpack-pwa-manifest": "^4.3.0",
    "whatwg-fetch": "3.6.2"
  }
}
